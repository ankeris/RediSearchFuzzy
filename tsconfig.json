{
    "compilerOptions": {
        "target": "es5",
        "module": "CommonJS",
        "declaration": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "strict": true,
        "baseUrl": "./",
        "noImplicitAny": true,
        "paths": {
            "@utils/*": ["src/@core/utils/*"],
            "@enums/*": ["src/@core/enums/*"],
            "@models/*": ["src/@core/models/*"],
            "@features/*": ["src/@features/*"]
        }
    }
}
